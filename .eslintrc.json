{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "import/extensions": [ 
        "error", 
        "ignorePackages", 
        { "": "never" } 
    ],
    "no-unused-vars": "off",
    "no-console": "off",
    "no-plusplus": "off",
    "no-new": 0,
    "class-methods-use-this": [
      "error", { 
        "exceptMethods": ["createWindow"] 
      }
    ]
  }
}